<h2>プロフィール編集</h2>
<%= form_with model: @user do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name, autofocus: true, placeholder: "名前" %>

  <%= f.label "ProfileImage" %>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>

  <!--変更を保存ボタン-->
  <%= f.submit "変更を保存" %>
<% end %>